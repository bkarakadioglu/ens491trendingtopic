{
    "config": {
      "view": {"continuousWidth": 1500, "continuousHeight": 500}  },
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
      "values": [
        
      ]
    },
    "transform": [
      {"flatten": ["sessionStart", "sessionEnd","sesLength","sessionMaxVolume"]}
    ],
    "mark": {"type": "circle", "tooltip": true},
    "encoding": {
      "x": { "field": "sessionStart", "type": "temporal","timeUnit":"utcyearmonthdatehoursminutes","axis": {"labelAngle": 45, "maxExtent":1000}},
      "y": {"field": "sesLength", "type": "quantitative","axis": {"offset":20}},
      "color": {
        "field":"name",
        "type":"nominal",
        "legend": null
      },
      "size": {
        "field": "sesLength",
        "type": "quantitative"
      },
      "tooltip": [
          {"field": "name", "type": "nominal"},
          {"field": "sessionStart", "type": "temporal","timeUnit":"utcyearmonthdatehoursminutes"},
          {"field": "sessionEnd", "type": "temporal","timeUnit":"utcyearmonthdatehoursminutes"},
          {"field": "sesLength", "type": "quantitative"},
          {"field": "sessionMaxVolume", "type": "quantitative"}
        ]
    }
  }